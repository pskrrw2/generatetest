{
  "SecurityHeaderSettings": {
    "Enable": true,
    "Headers": {
      "XFrameOptions": "SAMEORIGIN",
      "XContentTypeOptions": "nosniff",
      "ReferrerPolicy": "same-origin",
      "PermissionsPolicy": "geolocation=(), camera=()",
      "SameSite": "",
      "XXssProtection": "1; mode=block",
      "ContentPolicy": {
        "default-src": [
          "'self'",
          "'unsafe-inline'"
        ],
        "script-src": [
          "'self'",
          "'unsafe-inline'",
          "'unsafe-eval'",
          "https://localhost:44355",
          "https://www.google.com",
          "https://www.gstatic.com"
        ],
        "style-src": [
          "'self'",
          "'unsafe-inline'",
          "https://fonts.googleapis.com"
        ],
        "style-src-elem": [
          "'self'",
          "'unsafe-inline'",
          "https://fonts.googleapis.com"
        ],
        "font-src": [
          "'self'",
          "https://fonts.gstatic.com"
        ],
        "connect-src": [
          "'self'",
          "http://localhost:53463",
          "https://localhost:44355",
          "wss://localhost:44343",
          "ws://localhost:53463"
        ],
        "script-src-elem": [
          "'self'",
          "'unsafe-inline'",
          "https://localhost:44355",
          "https://www.google.com",
          "https://www.gstatic.com"
        ],
        "frame-src": [
          "'self'",
          "https://www.google.com"
        ],
        "img-src": [
          "'self' data:"
        ]
      }
    }
  }
}
